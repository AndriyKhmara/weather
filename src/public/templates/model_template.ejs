<div class="content">

    <%
    _.each(it.model, function (model) {        %>
    <h1><%- model.city %></h1>
    <p style="font-size: 10px;">Weather in: <%- model.time %></p>
    <ul>
        <li>Temp: <%- (model.main.temp - 273.15).toFixed(2) %></li>
        <li>Humidity: <%- model.main.humidity %></li>
        <li>Pressure: <%- model.main.pressure %></li>
        <li>Wind speed: <%- model.wind.speed %>, deg: <%- model.wind.deg %></li>
        <li>Weather: <%- model.weather[0].description %></li>
    </ul>

    <% }); %>

</div>